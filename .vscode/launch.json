{
    "version": "0.2.0",
    "projectName": "proj2",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/stm32l476/app/pwm_app/pwm_app",
            "name": "DEBUG PWM L4",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32L476",
            "svdFile": "${workspaceRoot}/mcu_support/l4xx/STM32L4x2.svd",
            "searchDir": [
                "${workspaceRoot}/mcu_support/stm32"
            ],
            "runToMain": true,
            "configFiles": [
                "${workspaceRoot}/mcu_support/stm32/stlink-dap.cfg",
                "${workspaceRoot}/mcu_support/stm32/l4xx/stm32l4x.cfg"
            ],
            "debuggerArgs": [
                "-d",
                "${workspaceRoot}",
            ],
            "preRestartCommands": [
                "add-symbol-file build/stm32l476/app/pwm_app/pwm_app",
                "load",
                "enable breakpoint",
                "monitor reset"
            ],
            "showDevDebugOutput": true,
        }
    ]
}